<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="root" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="0.0" styleClass="submission" xmlns="http://javafx.com/javafx/8.0.222" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox fx:id="textPane" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="title" styleClass="title" text="Title" wrapText="true" />
            <Label fx:id="subreddit" text="Subreddit">
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding></Label>
         </children>
      </VBox>
   </top>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <bottom>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <HBox maxWidth="1.7976931348623157E308">
               <children>
                  <Label fx:id="scoreNumber" styleClass="score-value" text="58" HBox.hgrow="NEVER" />
                  <Label styleClass="score-text" text="pts" HBox.hgrow="NEVER">
                     <padding>
                        <Insets right="10.0" />
                     </padding></Label>
                  <Label fx:id="commentsNumber" styleClass="score-value" text="12" HBox.hgrow="NEVER" />
                  <Label styleClass="score-text" text="comments" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Label>
                  <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                  <Label fx:id="date" styleClass="date" text="1d" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Label>
                  <Label fx:id="username" styleClass="username" text="username" />
               </children>
            </HBox>
            <HBox maxWidth="1.7976931348623157E308">
               <children>
                  <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="↑" HBox.hgrow="ALWAYS" />
                  <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="↓" HBox.hgrow="ALWAYS" />
                  <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="★" HBox.hgrow="ALWAYS" />
                  <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="⋮" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
   </bottom>
   <center>
      <VBox fx:id="contentPane" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
